

<%- include('partials/header.ejs') %>

<%- include('nav.ejs') %>   <!-- same folder mai jo nav file hai use add kra -->


<section>

    <h1 align="center"> Admin Default page </h1>


    <table class="table table-bordered">
        <thead class="table-dark">
            <tr>
                <th>User ID </th>
                <th>User Name </th>
                <th>User Age </th>
            </tr>
        </thead>
        <tbody>
            <% userData.forEach( (user) => {   %>
                <tr>
                    <td> <%= user.uid %> </td>
                    <td> <%= user.unm %> </td>
                    <td> <%= user.uage %> </td>
                </tr>

            <%    } ) %>
        </tbody>
    </table>


</section>

<%- include('partials/footer.ejs') %>
