
<%- include('./partials/header.ejs') %>

<%- include('./nav.ejs') %>

<!-- section ek part ke liye (ek part ki designing ke liye) -->
 <!-- section page specific designing krta h -->
<section>

    <table class="table table-bordered table-striped w-75 mx-auto mt-3 text-center">
        <thead class="table-dark">
            <tr>
                <th>User Name </th>
                <th>Password </th>
                <th>Email </th>
                <th>Role </th>
                <th>Active </th>
                <th>Date of reg </th>
            </tr>
        </thead>
        <tbody>
            <% userData.forEach( (user)=> {  %>
                <tr>
                    <td> <%= user.userName %> </td>
                    <td> <%= user.password %> </td>
                    <td> <%= user.emailId %> </td>
                    <td> <%= user.hasRole %> </td>
                    <td> <%= user.isActive %> </td>
                    <td> <%= user.createdAt %> </td>
                </tr>
            <% } ) %>
        </tbody>
    </table>



    
</section>

<%- include('./partials/footer.ejs') %>